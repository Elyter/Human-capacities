generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model ReflexResult {
  id           Int      @id @default(autoincrement())
  reactionTime Float
  timestamp    DateTime @default(now())
}

model NumberMemoryResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
}

model VisualMemoryResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
}

model VerbalMemoryResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
}

model ChimpTestResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
}

model SymbolMemoryResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
}

model SequenceMemoryResult {
  id        Int      @id @default(autoincrement())
  score     Int
  timestamp DateTime @default(now())
} 